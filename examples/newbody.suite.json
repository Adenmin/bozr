[
  {
    "name": "SparseObjectMatches",
    "calls": [
      {
        "on": {
          "method": "POST",
          "url": "https://postman-echo.com/post",
          "headers": {
            "Content-Type": "application/json"
          },
          "body": {
            "index": 1,
            "name": "Example #1"
          }
        },
        "expect": {
          "statusCode": 200,
          "newBody": {
            "data": {
              "index": 1
            }
          }
        }
      }
    ]
  },
  {
    "name": "SparseObjectDoesntMatch",
    "calls": [
      {
        "on": {
          "method": "POST",
          "url": "https://postman-echo.com/post",
          "headers": {
            "Content-Type": "application/json"
          },
          "body": {
            "index": 1,
            "name": "Example #1"
          }
        },
        "expect": {
          "statusCode": 200,
          "newBody": {
            "data": {
              "index": 1,
              "position": "LEFT"
            }
          }
        }
      }
    ]
  },
  {
    "name": "SparseArrayMatch",
    "calls": [
      {
        "on": {
          "method": "POST",
          "url": "https://postman-echo.com/post",
          "headers": {
            "Content-Type": "application/json"
          },
          "body": {
            "id": 9,
            "items": [
              "Weekly",
              "Monthly"
            ]
          }
        },
        "expect": {
          "statusCode": 200,
          "newBody": {
            "data": {
              "items": [
                "Weekly"
              ]
            }
          }
        }
      }
    ]
  }
]